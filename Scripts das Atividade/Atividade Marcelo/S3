enable
configure terminal
hostname S3
banner motd "ACESSO APENAS PARA PESSOAS AUTORIZADAS"
enable secret SenhadaEnable
ip domain-name peachanddaisy.local
crypto key generate rsa general-keys modulus 1024
username maurocortez privilege 15 secret Otirano
username estagiario privilege 1 secret SenhaEst
line console 0
password SenhadaConsole
login
exit
service password-encryption
line vty 0 15
password SenhadaVTY
transport input ssh
exec-timeout 5
login local
exit
vlan 80
name FINANCAS
exit
vlan 60
name OPERACOES
exit
vlan 88 
name RELOGIOS
exit
vlan 100
name LIVRE
exit
int range f0/1-5
switchport mode access
switchport access vlan 80
exit
int range f0/6-9
switchport mode access
switchport access vlan 60
exit
int range f0/10-19
switchport mode access
switchport access vlan 88
exit
int range f0/20-24
switchport mode access
switchport access vlan 100
exit
interface gig0/1
switchport mode trunk
switchport trunk allowed vlan 80,60,88,100
switchport trunk native vlan 88
exit
interface vlan 80
ip address 192.168.35.254 255.255.252.0
description FINANCAS
no shutdown
exit
interface vlan 60
ip address 192.168.37.254 255.255.254.0
description OPERACOES
no shutdown
exit
interface vlan 88
ip address 192.168. 38.254 255.255.255.0
description RELOGIOS
no shutdown
exit
interface vlan 100
ip address 192.168.39.62 255.255.255.192
description LIVRE
no shutdown
