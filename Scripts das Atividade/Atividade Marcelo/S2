enable
configure terminal
hostname S2
banner motd "ACESSO APENAS PARA PESSOAS AUTORIZADAS"
enable secret SenhadaEnable
ip domain-name peachanddaisy.local
crypto key generate rsa general-keys modulus 1024
username maurocortez privilege 15 secret Otirano
username estagiario privilege 1 secret SenhaEst
line console 0
password SenhadaConsole
login
exit
service password-encryption
line vty 0 15
password SenhadaVTY
transport input ssh
exec-timeout 5
login local
exit
vlan 50
name ANALISTAS
exit
vlan 60
name OPERACAO
exit
vlan 70
name TI
exit
vlan 80
name ADMINISTRATIVO
exit
int range f0/1-20
switchport mode access
switchport access vlan 50
exit
interface f0/24
switchport mode access
switchport access vlan 60
exit
int range f0/21,f0/23
switchport mode access
switchport access vlan 70
exit
interface f0/22
switchport mode access
switchport access vlan 80
exit
interface gig0/1
switchport mode trunk
switchport trunk allowed vlan 50,60,70,80
switchport trunk native vlan 88
exit
interface vlan 50
ip address 10.0.127.254 255.255.128.0
description ANALISTAS
no shutdown
exit
interface vlan 60
ip address 10.0.159.254 255.255.224.0
description OPERACAO
no shutdown
exit
interface vlan 70
ip address 10.0.167.254 255.255.248.0
description TI
no shutdown
exit
interface vlan 80
ip address 10.0.169.254 255.255.254.0
description ADMINISTRATIVO
no shutdown
do wr