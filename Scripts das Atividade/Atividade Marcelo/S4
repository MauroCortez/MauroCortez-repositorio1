enable
configure terminal
hostname S4
banner motd "ACESSO APENAS PARA PESSOAS AUTORIZADAS"
enable secret SenhadaEnable
ip domain-name peachanddaisy.local
crypto key generate rsa general-keys modulus 1024
username maurocortez privilege 15 secret Otirano
username estagiario privilege 1 secret SenhaEst
line console 0
password SenhadaConsole
login
exit
service password-encryption
line vty 0 15
password SenhadaVTY
transport input ssh
exec-timeout 5
login local
exit
vlan 88
name SERVIDORES
exit
vlan 200
name VOZ
exit
vlan 90
name TACACS
exit
vlan 88
name MONITORAMENTO
exit
int range f0/1-9
switchport mode access
switchport access vlan 88
exit
int range f0/10-12
switchport mode access
switchport access vlan 200
exit
int range f0/13-14
switchport mode access
switchport access vlan 90
exit
int range f0/15-24
switchport voice vlan 88
exit
interface gig0/1
switchport mode trunk
switchport trunk allowed vlan 88,90,200
switchport trunk native vlan 88
exit
interface vlan 88
ip address 172.27.255.254 255.252.0.0
description SERVIDORES
no shutdown
exit
interface vlan 200
ip address 172.28.127.254 255.255.128.0
description VOZ
no shutdown
exit
interface vlan 90
ip address 172.28.129.254 255.255.254.0
description TACACS
no shutdown
exit
interface vlan 88
ip address 172.23.255.254 255.248.0.0
description MONITORAMENTO
no shutdown
